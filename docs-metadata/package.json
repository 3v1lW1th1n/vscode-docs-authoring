{
	"name": "docs-metadata",
	"displayName": "docs-metadata",
	"description": "Docs Metadata",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extractMetadata"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extractMetadata",
				"title": "Extract Metadata",
				"category": "Docs"
			}
		],
		"commandPalette": [
			{
				"command": "extractMetadata",
				"group": "Docs"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/recursive-readdir": "^2.2.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"glob": "^7.1.3",
		"recursive-readdir": "^2.2.2"
	}
}
