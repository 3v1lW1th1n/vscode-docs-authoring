{
	"name": "docs-metadata",
	"displayName": "Docs Metadata",
	"description": "Simplifies editing metadata within docs.",
	"version": "0.0.1",
	"publisher": "docsmsft",
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.extract",
		"onCommand:extension.sayHello"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.extract",
				"title": "Docs Metadata: Extract"
			},
			{
				"command": "extension.sayHello",
				"title": "Say Hello"
			}
		],
		"menus": {
			"commandPalette": [
			  {
				"command": "extension.extract"
			  }
			]
		  }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
